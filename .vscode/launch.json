{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "philo",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/philo",
            "args": ["5", "500", "100", "100", "10"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
